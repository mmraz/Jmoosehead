<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
                                         "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
 <session-factory name="org.mraz.moosehead.util.SessionFactoryUtil">
  <property name="hibernate.connection.driver_class">org.gjt.mm.mysql.Driver</property>
  <property name="hibernate.connection.password">whocares</property>
  <property name="hibernate.connection.url">jdbc:mysql://10.0.0.102:3306/moose</property>
  <property name="hibernate.connection.username">moose</property>
  <property name="hibernate.dialect">org.hibernate.dialect.MySQL5Dialect</property>
  <property name="hibernate.connection.autocommit">true</property>
  <property name="hibernate.show_sql">true</property>
  <property name="hibernate.use_sql_comments">true</property>
  <property name="hibernate.format_sql">true</property>
  <mapping class="org.mraz.moosehead.pojo.CharacterPojo" resource="CharacterPojo.hbm.xml"/>
 </session-factory>
</hibernate-configuration>
