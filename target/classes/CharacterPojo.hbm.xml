<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="CharacterPojo" table="player">
  <id column="id" name="identity" type="long">
   <generator class="increment"/>
  </id>
  <property generated="never" lazy="false" name="timestamp" type="timestamp">
   <column name="timestamp" sql-type="TIMESTAMP"/>
  </property>
 </class>
</hibernate-mapping>
